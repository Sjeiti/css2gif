{
  "name": "css2gif",
  "version": "1.1.6",
  "description": "Mc Picker, a minimal color picker",
  "author": "Ron Valstar",
  "main": "dist/index.js",
  "files": [
    "dist/index.js"
  ],
  "scripts": {
    "start": "run-p build serve",
    "build": "run-s clean \"webpack -- --env.development --watch\"",
    "build:prod": "run-s clean \"webpack -- --env.production\"",
    "webpack": "npx webpack --config webpack.config.js",
    "clean": "del-cli dist -f",
    "serve": "node task/serve docs 7552",
    "version": "next-version package.json",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "dependencies": {
    "animated_gif": "^1.0.2",
    "mc_picker": "^1.1.12"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/preset-env": "7.2.0",
    "autoprefixer": "^9.7.1",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "body-parser": "^1.18.2",
    "chokidar-cli": "^2.0.0",
    "css-loader": "^1.0.0",
    "css-properties-values": "^1.0.0",
    "del-cli": "^1.1.0",
    "eslint": "^4.18.2",
    "express": "^4.16.3",
    "filemanager-webpack-plugin": "^2.0.5",
    "html-loader": "^0.5.5",
    "html-minifier": "^4.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "less-plugin-glob": "^3.0.0",
    "next-version": "^0.11.7",
    "npm-run-all": "^4.1.5",
    "open": "^6.4.0",
    "rimraf": "^2.6.1",
    "serve-static": "^1.13.2",
    "style-loader": "^0.23.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "yorkie": "^2.0.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "gitHooks": {
    "pre-commit": "npm run version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sjeiti/css2gif.git"
  },
  "keywords": [
    "colorpicker",
    "color",
    "ui",
    "rgb",
    "minimal",
    "small"
  ],
  "homepage": "https://sjeiti.github.io/css2gif/"
}
